{
  "name": "@fmidev/smartmet-alert-client",
  "version": "3.9.7",
  "description": "Web application for viewing weather and flood alerts",
  "author": "Finnish Meteorological Institute",
  "license": "MIT",
  "main": "dist/SmartMetAlertClient.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmidev/smartmet-alert-client.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "build": "vue-cli-service build --target lib --name SmartMetAlertClient --skip-plugins @vue/cli-plugin-eslint && rm ./dist/demo.html",
    "lint": "vue-cli-service lint",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@panzoom/panzoom": "4.5.1",
    "@xmldom/xmldom": "0.8.6",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "2.23.1",
    "cross-fetch": "3.1.5",
    "focus-visible": "5.2.0",
    "he": "1.2.0",
    "isomorphic-fetch": "3.0.0",
    "mapshaper": "0.5.26",
    "mutationobserver-shim": "0.3.7",
    "svgpath": "2.6.0",
    "url-search-params-polyfill": "8.1.1",
    "vue": "2.7.14",
    "vue-i18n": "8.28.2",
    "vue-observe-visibility": "1.0.0",
    "vue-window-size": "0.6.2",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.16.0",
    "@babel/plugin-transform-classes": "7.21.0",
    "@vue/cli": "5.0.8",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-standard": "8.0.1",
    "babel-eslint": "10.1.0",
    "copy-webpack-plugin": "11.0.0",
    "core-js": "3.29.1",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-webpack-plugin": "4.0.0",
    "find-node-modules": "2.1.3",
    "popper.js": "1.16.1",
    "portal-vue": "2.1.7",
    "postcss-prefixwrap": "1.38.1",
    "prettier": "2.8.4",
    "sass": "1.58.1",
    "sass-loader": "10.4.1",
    "string.prototype.replaceall": "1.0.7",
    "svg64": "1.2.0",
    "vite": "4.1.1",
    "vitest": "0.28.5",
    "vue-cli-plugin-bootstrap-vue": "0.8.2",
    "vue-eslint-parser": "8.0.1",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "webpack": "5.76.2",
    "xml-formatter": "2.6.1",
    "xmlhttprequest-ssl": "2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@vue/standard",
      "plugin:vue/recommended",
      "plugin:sonarjs/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "semi": false
  },
  "bugs": {
    "url": "https://github.com/fmidev/smartmet-alert-client/issues"
  },
  "homepage": "https://github.com/fmidev/smartmet-alert-client#readme",
  "directories": {
    "test": "test"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=18.0.0"
  },
  "keywords": [
    "smartmet",
    "weather",
    "alert",
    "wfs"
  ]
}
