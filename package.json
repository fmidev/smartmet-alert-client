{
  "name": "@fmidev/smartmet-alert-client",
  "version": "4.0.0-alpha.32",
  "description": "Web application for viewing weather and flood alerts",
  "author": "Finnish Meteorological Institute",
  "license": "MIT",
  "main": "dist/SmartMetAlertClient.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmidev/smartmet-alert-client.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test": "vitest"
  },
  "dependencies": {
    "@panzoom/panzoom": "4.5.1",
    "@xmldom/xmldom": "0.8.10",
    "bootstrap": "5.3.1",
    "bootstrap-vue-next": "0.12.3",
    "cross-fetch": "3.1.5",
    "flatbush": "3.2.1",
    "dompurify": "3.0.5",
    "focus-visible": "5.2.0",
    "he": "1.2.0",
    "svgpath": "2.6.0",
    "url-search-params-polyfill": "8.2.5",
    "vue": "3.3.4",
    "vue-observe-visibility": "1.0.0",
    "xpath": "0.0.33"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.3.4",
    "@vue/eslint-config-standard": "8.0.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "9.17.0",
    "prettier": "3.0.3",
    "rollup-plugin-visualizer": "5.9.2",
    "unplugin-vue-components": "0.25.2",
    "vite": "4.4.9",
    "vite-plugin-banner": "0.7.0",
    "postcss-prefixwrap": "1.40.0",
    "vitest": "0.34.4"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "@vue/standard",
      "plugin:vue/recommended",
      "plugin:sonarjs/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "semi": false
  },
  "bugs": {
    "url": "https://github.com/fmidev/smartmet-alert-client/issues"
  },
  "homepage": "https://github.com/fmidev/smartmet-alert-client#readme",
  "directories": {
    "test": "test"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=18.0.0"
  },
  "keywords": [
    "smartmet",
    "weather",
    "alert",
    "wfs"
  ]
}
