{
  "name": "@fmidev/smartmet-alert-client",
  "version": "3.6.6",
  "description": "Web application for viewing weather and flood alerts",
  "author": "Finnish Meteorological Institute",
  "license": "MIT",
  "main": "dist/SmartMetAlertClient.umd.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmidev/smartmet-alert-client.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "build": "vue-cli-service build --target lib --name SmartMetAlertClient --skip-plugins @vue/cli-plugin-eslint && rm ./dist/demo.html",
    "lint": "vue-cli-service lint",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@formatjs/intl-datetimeformat": "6.10.0",
    "@formatjs/intl-getcanonicallocales": "2.2.1",
    "@formatjs/intl-locale": "3.3.2",
    "@formatjs/intl-numberformat": "8.7.0",
    "@formatjs/intl-pluralrules": "5.2.4",
    "@panzoom/panzoom": "4.4.3",
    "@xmldom/xmldom": "0.8.6",
    "bootstrap": "4.6.1",
    "bootstrap-vue": "2.22.0",
    "cross-fetch": "3.1.5",
    "focus-visible": "5.2.0",
    "he": "1.2.0",
    "isomorphic-fetch": "3.0.0",
    "mapshaper": "0.5.26",
    "mutationobserver-shim": "0.3.7",
    "svgpath": "2.5.0",
    "url-search-params-polyfill": "8.1.1",
    "vue": "2.6.14",
    "vue-i18n": "8.26.7",
    "vue-observe-visibility": "1.0.0",
    "vue-window-size": "0.6.2",
    "vuex": "3.6.2",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.16.0",
    "@babel/plugin-transform-classes": "7.16.0",
    "@vue/cli": "4.5.15",
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-plugin-vuex": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-standard": "5.1.2",
    "babel-eslint": "10.1.0",
    "copy-webpack-plugin": "6.4.0",
    "cross-env": "7.0.3",
    "eslint": "7.8.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "7.20.0",
    "find-node-modules": "2.1.2",
    "popper.js": "1.16.1",
    "portal-vue": "2.1.7",
    "postcss-prefixwrap": "1.25.0",
    "prettier": "2.4.1",
    "sass": "1.32.13",
    "sass-loader": "10.0.5",
    "string.prototype.replaceall": "1.0.5",
    "svg64": "1.1.0",
    "vite": "4.0.0",
    "vitest": "0.25.6",
    "vue-cli-plugin-bootstrap-vue": "0.8.2",
    "vue-eslint-parser": "8.0.1",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.46.0",
    "xml-formatter": "2.6.1",
    "xmlhttprequest-ssl": "2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@vue/standard",
      "plugin:vue/recommended",
      "plugin:sonarjs/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "semi": false
  },
  "bugs": {
    "url": "https://github.com/fmidev/smartmet-alert-client/issues"
  },
  "homepage": "https://github.com/fmidev/smartmet-alert-client#readme",
  "directories": {
    "test": "test"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": "14.0.0 - 17.0.0"
  },
  "keywords": [
    "smartmet",
    "weather",
    "alert",
    "wfs"
  ]
}
